@model List<ListItemViewModel>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@employee.Name</td>
                <td>@employee.LastName</td>
                <td>@employee.FatherName</td>
            </tr>
        }
    </tbody>
</table>